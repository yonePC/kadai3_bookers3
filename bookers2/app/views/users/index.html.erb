

<div class="container px-5 px-sm-0">
  <div class="row">
    <div class="col-md-3">
    <!--部分テンプレート-->
    <%= render "users/userinfo", user: @users %>


    <div class="col-md-8 offset-md-1">
      <h2>Users</h2>
      <table class="table">
        <thead>
         <tr>
          <th>image</th>
          <th>name</th>
          <th colspan="3"></th>
         </tr>
        </thead>

        <tbody>
        <% @users.each do |user| %>
         <tr>
          <td><img class="attachment user profile_image fallback" src="/assets/no-image-icon-d3f9e11cb2bb284b42e0810f94b03c50126f51f482ea43054b87c2b7ebaa01d5.jpg"></td>
          <td><%= user.name %></td>
          <td>
           <%= link_to 'Show', users_path(@users) %>
          </td>
         </tr>
        <% end %>
        </tbody>
      </table>
    </div>
   </div>
</div>